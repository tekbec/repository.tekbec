<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="plugin.video.nhl66" name="NHL66" version="1.0.6" provider-name="tekbec">
  <requires>
    <import addon="xbmc.python" version="3.0.0" />
    <import addon="script.module.codequick" version="1.0.3" />
    <import addon="script.module.requests" version="2.31.0" />
    <import addon="script.module.pysocks" version="1.7.0+matrix.1" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
   <extension point="xbmc.service" library="service.py" />
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary>NHL Streams</summary>
    <description>Watch Free NHL Streams. This service offers live streams and replays.</description>
    <assets>
        <icon>resources/media/icon.png</icon>
        <fanart>resources/media/fanart.jpg</fanart>
    </assets>
    <website>https://nhl66.ir/</website>
    <source>https://github.com/tekbec/plugin.video.nhl66</source>
  </extension>
</addon><addon id="repository.tekbec" name="tecbec's Repository" version="1.0.0" provider-name="tekbec">
    <extension point="xbmc.addon.repository" name="tecbec's Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/tekbec/repository.tekbec/main/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/tekbec/repository.tekbec/main/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/tekbec/repository.tekbec/main/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>tecbec's Repository</summary>
        <description>Quebec And Hockey Addons</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon></addons>